{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Train",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/train.py",
        "args": [
          "-m",
          "2",
          "--name",
          "attention_bayes",
          "-o",
          "0",
          "--bayes_opt",
          "True",
          "-s2",
          "192",
          "--stridetrain",
          "64",
          "--delta",
          "1e-4",
          "--patience",
          "5",
          "--loss_function",
          "0",
          "--folder",
          "bayes"
        ],
        "console": "integratedTerminal",
        "cwd": "${workspaceFolder}",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "stopOnEntry": false
      }
    ]
  }
  